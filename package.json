{
  "name": "ols-termpopup",
  "description": "This widget displays a pop up window with information about an ontology term. The widget is powered by the ontology Lookup service",
  "version": "0.0.1",
  "homepage": "https://github.com/LLTommy/ols-termpopup",
  "author": {
    "name": "lltommy",
    "email": "lltommy@felix-solution.com"
  },
