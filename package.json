    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "echo 'Error: no test specified' && exit 1",
    "build": "mkdirp build && browserify -r ./:ols-termpopup -o build/ols-termpopup.js",
    "watch": "mkdirp build && watchify -r ./:ols-termpopup -o build/ols-termpopup.js",
    "build-browser": "npm run build",
    "install": "npm run build",
    "uglify": "gulp uglify"
