    "gulp-uglify": "^1.5.1",
    "mkdirp": "^0.5.0",
    "watchify": "^3.7.0"
  },
  "keywords": [
    "ontology",
    "info window",
    "ontology pop up",
    "ebi",
    "biojs",
    "ols",
    "network",
    "Ontology Lookup Service"
  ]
